{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "ecmaFeatures": {
        "classes": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 7
    },
    "rules": {
        "max-len":[2, 120, 4, { "ignoreUrls": true }],
        "no-multi-spaces": 0
    }
}
